{
  "name": "sokcheat-framework-css",
  "version": "1.0.0",
  "description": "A modern, utility-first CSS framework with cutting-edge features",
  "main": "dist/sokcheat-framework.css",
  "style": "dist/sokcheat-framework.css",
  "files": ["dist/", "src/", "README.md", "LICENSE"],
  "scripts": {
