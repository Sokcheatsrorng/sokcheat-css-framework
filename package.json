  "keywords": ["css", "framework", "utility-first", "responsive", "modern", "sokcheat-framework"],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sokcheatsrorng/sokcheat-css-framework.git"
  },
  "homepage": "https://github.com/Sokcheatsrorng/sokcheat-css-framework#readme",
  "jsdelivr": "dist/sokcheat-framework.css",
  "unpkg": "dist/sokcheat-framework.css",
  "devDependencies": {
    "sass": "^1.69.0"
  }
}
